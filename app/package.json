{
  "name": "portfolio-server",
  "version": "1.0.0",
  "description": "Portfolio site using Express, Vue and Mongo",
  "scripts": {
    "serve:client": "vue-cli-service serve --silent",
    "watch:client": "vue-cli-service build --watch --silent",
    "dev:server": "nodemon --ignore './public/' --watch './server' main.js",
    "dev": "concurrently -k \"npm run serve:client\" \"npm run dev:server\"",
    "build": "vue-cli-service build",
    "start": "node server/main.js"
  },
  "keywords": [
    "site",
    "portfolio",
    "photo"
  ],
  "author": "ratiih",
  "license": "MIT",
  "devDependencies": {
    "morgan": "^1.9.1",
    "nodemon": "^1.19.0",
    "concurrently": "5.0.2",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-unit-mocha": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "dotenv-flow": "^2.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "formidable": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.8",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "socket.io": "^2.2.0",
    "@ckeditor/ckeditor5-build-classic": "^12.2.0",
    "@ckeditor/ckeditor5-vue": "^1.0.0-beta.2",
